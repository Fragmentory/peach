    "C_Cpp.vcFormat.indent.braces": true,
    "C_Cpp.clang_format_sortIncludes": true,
    "cmake.buildDirectory": "${workspaceFolder}/bin",
    "doxdocgen.generic.useGitUserName": true,
    "doxdocgen.generic.useGitUserEmail": true,
    "doxdocgen.generic.dateFormat": "MMMM DD, YYYY",
    "doxdocgen.file.copyrightTag": [
        "Copyright (c) {year} Roman Koch, koch.roman@gmail.com",
        "SPDX-License-Identifier: MIT"
    ],
    "doxdocgen.file.fileTemplate": "\\file {name}",
    "doxdocgen.generic.authorEmail": "{name}",
    "doxdocgen.generic.authorName": "{email}",
    "doxdocgen.generic.authorTag": "\\author {author} ({email})",
    "doxdocgen.generic.briefTemplate": "\\brief {text}",
    "doxdocgen.generic.dateTemplate": "\\date {date}",
    "doxdocgen.generic.paramTemplate": "\\param {param} ",
    "doxdocgen.generic.returnTemplate": "\\return {type} ",
    "doxdocgen.file.fileOrder": [
        "file",
        "author",
        "empty",
        "copyright"
    ],
    "doxdocgen.generic.order": [
        "brief",
        "empty",
        "tparam",
        "param",
        "return",
    ],
    "files.associations": {},
    "workbench.preferredDarkColorTheme": "Default Dark Modern",
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.colorCustomizations": {
        "[Default Dark Modern]": {
            "tab.activeBackground": "#3f3f3f",
            "tab.activeBorder": "#5f5f5f",
            "tab.hoverBorder": "#5f5f5f",
            "editor.background": "#0e0e0f",
            "terminal.background": "#0e0e0f",
            "outputView.background": "#0e0e0f"
        }
    },
    "r.lsp.promptToInstall": false,
    "C_Cpp.errorSquiggles": "enabled",
    "cmake.options.statusBarVisibility": "visible",
    "cmake.configureOnOpen": false,
    "svg.preview.background": "transparent",
    "cortex-debug.variableUseNaturalFormat": true,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.autoAddFileAssociations": false,
    "terminal.integrated.bellDuration": 10000
}